type Idea = 
 record {
   "desc": text;
   "idea": text;
   "link": text;
 };
type Event = 
 record {
   "countryCode": text;
   "ip": text;
   "language": text;
   "lat": text;
   "lon": text;
   "platform": text;
   "region": text;
   "resolution": text;
   "timestamp": nat;
   "userAgent": text;
 };
service : {
  "getIdea": (Event) -> (Idea);
  "getLogs": () -> (vec Event) query;
}
